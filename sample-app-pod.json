{
    "kind": "Pod",
    "apiVersion": "v1",
    "metadata":{
        "name": "sample-app",
        "namespace": "gocd",
        "labels": {
            "name": "sample-app"
        }
    },
    "spec": {
        "containers": [{
            "name": "sample-app",
            "image": "varshavs/sample-app:#{tag}",
